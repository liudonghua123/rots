<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.ba.mapper.BaSkjhMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  ba_skjh 
		WHERE
			 htid = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.ba.entity.BaSkjh">
		SELECT * 
		FROM  ba_skjh
		WHERE
			 htid = #{mainId} 	</select>
</mapper>
